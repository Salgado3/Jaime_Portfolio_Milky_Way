---
interface Props {
    url: string;
    image: string;
    title: string;
    body: string;
}

const { url, image, title, body, } = Astro.props;
---

<li class="card">
    <a class="card__link" href={url}>
        <div class="center">

        </div>
        <h3 class="card__title">
            {title}
        </h3>
        <p class="card__txt">
            {body}
        </p>
    </a>
</li>
